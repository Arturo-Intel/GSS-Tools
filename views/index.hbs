{{#unless home}}
(async () => {
    
    const saveHit = await fetch("/hit", {
            method: "POST",
            headers: {
                'Content-Type': 'application/json' // Set the content type to JSON
            },
            body: JSON.stringify({
                "datetime": getDateTime(),
                "action": "Landing page",
                "result": "OK"
            }),
        });
    if (!saveHit.ok) {
        console.error("Could not save hit data", saveHit.status);
    } 
})();
{{/unless}}
</script>

{{#if isAuthenticated }}
<h1>Welcome {{profile.givenName}}!</h1>

<ul>
    {{#each profile}}

    <li>{{@key}}: {{this}}</li>

    {{/each}}
</ul>
{{else}}
<h1>One moment please...</h1>

{{/if}}
